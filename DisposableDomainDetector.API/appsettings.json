{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "AllowedHosts": "*",
  "DisposableDomains": {
    "BaseUrl": "https://raw.githubusercontent.com/disposable/disposable-email-domains/master/",
    "ExpiryInMinutes": 1440,
    "Timeout": 10000
  }
}
